# Calendly Integration Environment Variables
# Copy this file to .env.local and update with your actual values

# Database connection string
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require

# Calendly API credentials
CALENDLY_API_KEY=your_calendly_api_key
CALENDLY_ORGANIZATION=your_calendly_organization_id
CALENDLY_USER=your_calendly_user_id

# Notification settings for migrations (optional)
NOTIFY_MIGRATION_SUCCESS=true
NOTIFY_MIGRATION_FAILURE=true