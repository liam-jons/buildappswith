# Production Environment Variables
# IMPORTANT: Don't commit sensitive values - set these in Vercel dashboard

# Application Configuration
NODE_ENV=production
NEXT_PUBLIC_API_URL=https://buildappswith.com/api
NEXT_PUBLIC_SITE_URL=https://buildappswith.com
NEXT_PUBLIC_APP_ENV=production

# Clerk Authentication
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/login
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/dashboard
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/onboarding

# Calendly Scheduling
CALENDLY_DEBUG_MODE=true
CALENDLY_ENABLE_CACHING=false

# Feature Flags
NEXT_PUBLIC_ENABLE_BUILDER_PROFILES=true
NEXT_PUBLIC_ENABLE_SKILL_TREE=false
NEXT_PUBLIC_ENABLE_TIMELINE=false
FEATURE_BUILDER_PROFILES=true
FEATURE_PORTFOLIO_SHOWCASE=true

# Datadog
NEXT_PUBLIC_DATADOG_SITE=datadoghq.eu
DATADOG_ENABLE_DEV=false

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_DEFAULT=60
RATE_LIMIT_WINDOW_SIZE=60

# Sanity Content Management
NEXT_PUBLIC_SANITY_DATASET="production"