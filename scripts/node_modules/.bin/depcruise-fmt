#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/liamj/Documents/Development/buildappswith/scripts/node_modules/.pnpm/dependency-cruiser@16.10.1/node_modules/dependency-cruiser/bin/node_modules:/Users/liamj/Documents/Development/buildappswith/scripts/node_modules/.pnpm/dependency-cruiser@16.10.1/node_modules/dependency-cruiser/node_modules:/Users/liamj/Documents/Development/buildappswith/scripts/node_modules/.pnpm/dependency-cruiser@16.10.1/node_modules:/Users/liamj/Documents/Development/buildappswith/scripts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/liamj/Documents/Development/buildappswith/scripts/node_modules/.pnpm/dependency-cruiser@16.10.1/node_modules/dependency-cruiser/bin/node_modules:/Users/liamj/Documents/Development/buildappswith/scripts/node_modules/.pnpm/dependency-cruiser@16.10.1/node_modules/dependency-cruiser/node_modules:/Users/liamj/Documents/Development/buildappswith/scripts/node_modules/.pnpm/dependency-cruiser@16.10.1/node_modules:/Users/liamj/Documents/Development/buildappswith/scripts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../dependency-cruiser/bin/depcruise-fmt.mjs" "$@"
else
  exec node  "$basedir/../dependency-cruiser/bin/depcruise-fmt.mjs" "$@"
fi
